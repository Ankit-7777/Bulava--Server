{
    "version":2,
    "builds":[
        {
            "src": "WeddingCard/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize":"15mb",
                "runtime": "python3.9"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "WeddingCard/wsgi.py"
        }
    ]
}